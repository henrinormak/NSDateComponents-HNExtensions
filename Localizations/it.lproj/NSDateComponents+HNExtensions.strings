/* Year, singular */
"year" = "anno";

/* Year, plural */
"year##plural" = "anni";

/* Month, singular */
"month" = "mese";

/* Month, plural */
"month##plural" = "mesi";

/* Week, singular */
"week" = "settimana";

/* Week, plural */
"week##plural" = "settimane";

/* Day, singular */
"day" = "giorno";

/* Day, plural */
"day##plural" = "iorni";

/* Hour, singular */
"hour" = "ora";

/* Hour, plural */
"hour##plural" = "ore";

/* Minute, singular */
"minute" = "minuto";

/* Minute, plural */
"minute##plural" = "minuti";

/* Second, singular */
"second" = "secondi";

/* Second, plural */
"second##plural" = "secondi";
